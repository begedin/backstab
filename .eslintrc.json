{
  "extends": [
    "eslint:recommended",
    "plugin:import/warnings",
    "plugin:import/errors",
    "airbnb/base",
    "prettier"
  ],
  "plugins": ["import", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
